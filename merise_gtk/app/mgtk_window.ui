<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.96.3 -->
<interface>
  <!-- interface-name mgtk_window.ui -->
  <requires lib="gtk" version="4.0"/>
  <requires lib="gio" version="2.0"/>
  <requires lib="libadwaita" version="1.4"/>
  <template class="mgtk-window" parent="GtkApplicationWindow">
    <property name="decorated">False</property>
    <property name="default-height">600</property>
    <property name="default-width">1400</property>
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="AdwHeaderBar" id="header">
            <property name="title-widget">
              <object class="AdwWindowTitle" id="title">
                <property name="title">MeriseGTK</property>
              </object>
            </property>
            <child>
              <object class="AdwSplitButton" id="open-btn">
                <property name="action-name">mgtk.open</property>
                <property name="label">Open</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="undo-btn">
                <property name="action-name">mgtk.undo</property>
                <property name="icon-name">edit-undo-symbolic</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="redo-btn">
                <property name="action-name">mgtk.redo</property>
                <property name="icon-name">edit-redo-symbolic</property>
              </object>
            </child>
            <child type="end">
              <object class="GtkMenuButton" id="menu-btn">
                <property name="focusable">True</property>
                <property name="icon-name">open-menu-symbolic</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
  <menu id="mgtk_menu"/>
</interface>
